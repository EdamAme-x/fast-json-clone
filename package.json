    "url": "https://github.com/rhysd/fast-json-clone/issues"
  },
  "homepage": "https://github.com/rhysd/fast-json-clone#readme",
  "devDependencies": {
    "@types/benchmark": "^2.1.2",
    "@types/clone-deep": "^4.0.1",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/ungap__structured-clone": "^0.3.0",
    "@ungap/structured-clone": "^1.0.1",
    "benchmark": "^2.1.4",
