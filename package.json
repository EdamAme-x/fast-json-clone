  "description": "Clone JSON value faster than the fastest",
  "main": "index.js",
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "tsc -p . -d",
    "watch": "tsc -p . -d -w",
